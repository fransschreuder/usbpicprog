#>+ 15
force-reedit:
		@for dep in $?; do \
	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh \
		&& exit 0; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --gnu  src/tools/ccsc/Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  src/tools/ccsc/Makefile
	cd $(top_srcdir) && perl admin/am_edit src/tools/ccsc/Makefile.in


#>+ 21
clean-bcheck: 
	rm -f *.bchecktest.cc *.bchecktest.cc.class a.out

bcheck: bcheck-recursive

bcheck-am:
	@for i in ; do \
	    if test $(srcdir)/$$i -nt $$i.bchecktest.cc; then \
	        echo "int main() {return 0;}" > $$i.bchecktest.cc ; \
	        echo "#include \"$$i\"" >> $$i.bchecktest.cc ; \
	        echo "$$i"; \
	        if ! $(CXXCOMPILE)  --dump-class-hierarchy -c $$i.bchecktest.cc; then \
	            rm -f $$i.bchecktest.cc; exit 1; \
	        fi ; \
	        echo "" >> $$i.bchecktest.cc.class; \
	        perl $(top_srcdir)/admin/bcheck.pl $$i.bchecktest.cc.class || { rm -f $$i.bchecktest.cc; exit 1; }; \
	        rm -f a.out; \
	    fi ; \
	done


#>+ 11
libccsc_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/ccsc.cpp $(srcdir)/ccsc_compile.cpp $(srcdir)/ccsc_config.cpp ccsc_compile.moc.cpp 
	@echo 'creating libccsc_la.all_cpp.cpp ...'; \
	rm -f libccsc_la.all_cpp.files libccsc_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libccsc_la.all_cpp.final; \
	for file in ccsc.cpp ccsc_compile.cpp ccsc_config.cpp ccsc_compile.moc.cpp ; do \
	  echo "#include \"$$file\"" >> libccsc_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libccsc_la.all_cpp.final; \
	done; \
	cat libccsc_la.all_cpp.final libccsc_la.all_cpp.files > libccsc_la.all_cpp.cpp; \
	rm -f libccsc_la.all_cpp.final libccsc_la.all_cpp.files

